{
  "name": "codex-for-windows",
  "version": "0.1.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "codex-for-windows",
      "version": "0.1.0",
      "license": "MIT",
      "workspaces": [
        "packages/*"
      ],
      "engines": {
        "node": ">=18.18"
      }
    },
    "node_modules/@codex-win/core": {
      "resolved": "packages/core",
      "link": true
    },
    "node_modules/@codex-win/diagnostics": {
      "resolved": "packages/diagnostics",
      "link": true
    },
    "node_modules/@codex-win/patches": {
      "resolved": "packages/patches",
      "link": true
    },
    "node_modules/codex-win": {
      "resolved": "packages/cli",
      "link": true
    },
    "packages/cli": {
      "name": "codex-win",
      "version": "0.1.0",
      "dependencies": {
        "@codex-win/core": "0.1.0",
        "@codex-win/diagnostics": "0.1.0"
      },
      "bin": {
        "codex-win": "bin/codex-win.js"
      }
    },
    "packages/core": {
      "name": "@codex-win/core",
      "version": "0.1.0",
      "dependencies": {
        "@codex-win/patches": "0.1.0"
      }
    },
    "packages/diagnostics": {
      "name": "@codex-win/diagnostics",
      "version": "0.1.0"
    },
    "packages/patches": {
      "name": "@codex-win/patches",
      "version": "0.1.0"
    }
  }
}
